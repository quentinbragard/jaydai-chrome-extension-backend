{
    "global_messages_avg_grade": {
        "type": "classic",
        "column": "grade_prompt_output",
        "operation": "custom",
        "custom_function": "mean_grade"
    },
    "max_grade": {
        "type": "classic",
        "column": "grade_prompt_output",
        "operation": "custom",
        "custom_function": "max_grade"
    },
    "min_grade": {
        "type": "classic",
        "column": "grade_prompt_output",
        "operation": "custom",
        "custom_function": "min_grade"
    },
    "total_number_of_messages": {
        "type": "classic",
        "column": "message_id",
        "operation": "count"
    },
    "total_number_of_user_messages": {
        "type": "classic",
        "column": "message_id",
        "operation": "count",
        "filter": {
            "filter_column": "role",
            "filter_value": "user"
        }
    },
    "total_number_of_assistant_messages": {
        "type": "classic",
        "column": "message_id",
        "operation": "count",
        "filter": {
            "filter_column": "role",
            "filter_value": "assistant"
        }
    },
    "total_cost": {
        "type": "classic",
        "column": "cost",
        "operation": "sum"
    },
    "total_energy_consumption": {
        "type": "classic",
        "column": "energy_consumption",
        "operation": "sum"
    },
    "assistant_grade_explanation": {
        "type": "ai_based",
        "source": "messages",
        "filter": {
            "filter_column": "role",
            "filter_value": "user"
        },
        "column": "explain_grade_output",
        "prompt_chains": [
            {
                "instructions_file": "analyzer_agent/prompts/agg_grade_explanation_instructions.txt",
                "prompt": "Grades explanations: {column}",
                "model": "gpt-4o-mini"
            }
        ]
    }
} 